<div class="container search">
    <div class="search-box">
        <div class="banner-area">
            <img src="/images/special/banner/{{special}}.png" alt="Wood" class="banner">
        </div>
        <div class="search-area">
            <form action="/search" method="get">
                <input type="search" name="q" id="q" placeholder="Search" required autofocus>
                <button type="submit" style="background-color: #{{special}} !important;"><box-icon
                        name='search'></box-icon></button>
            </form>
        </div>
    </div>
</div>
<script>
    $(function () {
        $("#q").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: "https://suggestqueries.google.com/complete/search",
                    dataType: "jsonp",
                    data: {
                        q: request.term,
                        client: "firefox",
                        ds: "yt"
                    },
                    success: function (data) {
                        let suggestion = []
                        console.log(data[1])
                        data[1].forEach(sugg => {
                            suggestion.push(sugg.slice(0, 10))
                        })
                        response(suggestion);
                    }
                });
            },
            minLength: 2
        });
    });
</script>