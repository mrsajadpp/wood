{{>search}}
{{#if images}}
<div class="containerr" style="min-height: 100vh !important;">
    {{#each images}}
    <div class="card" onclick="location.href = '{{this}}';">
        <div class="imgContainer">
            <img src="{{this}}" alt="Image" onerror="this.onerror=null;this.src='https://i.postimg.cc/gcMZfD83/20230318-161058.jpg';" />
        </div>
    </div>
    {{/each}}
</div>
{{>chaticon}}
{{else}}
<div class="container" style="display: flex; align-items: center; justify-content: center; width: 100%; height: 100vh;">
    <div class="notfound">
        <span class="text">No results <span class="green-text">found</span>!</span>
    </div>
</div>
{{/if}}
